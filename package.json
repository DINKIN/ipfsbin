    "babel-preset-stage-0": "^6.5.0",
    "connect": "^3.4.1",
    "css-loader": "^0.23.1",
    "html-webpack-plugin": "^2.9.0",
    "ipfs-api": "^2.13.1",
    "ipfsd-ctl": "^0.8.3",
    "json-loader": "^0.5.4",
    "morgan": "^1.7.0",
    "offline-plugin": "^2.0.4",
